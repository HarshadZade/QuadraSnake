<launch>
    <!-- Start tilt joint controller -->
    <rosparam file="$(find my_dynamixel_tutorial)/both.yaml" command="load"/>
    <node name="tilt_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port pan_tilt_port
                motor1
		motor2
		motor3
		motor4
		motor5
		motor6
		motor7
		motor8
		motor9
		motor10
		motor11
		motor12
		motor13
		motor14"
          output="screen"/>
</launch>
